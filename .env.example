# Copy this file to .env and fill in your values.
# Required for Firebase (patient details, logs, alerts):

# Path to the service account JSON key (relative to project root or absolute).
# Default: serviceAccountKey.json in project root
FIREBASE_CREDENTIALS_PATH=serviceAccountKey.json

# Your Firebase Storage bucket (optional; only needed for discharge document uploads).
# Format: your-project-id.appspot.com (find in Firebase Console > Project Settings > General)
FIREBASE_STORAGE_BUCKET=your-project-id.appspot.com

# App secrets (use your own values in production)
SECRET_KEY=your-secret-key-here
JWT_SECRET_KEY=your-jwt-secret-key-here

# Optional: Qdrant (for RAG / discharge doc search)
QDRANT_HOST=localhost
QDRANT_PORT=6333
QDRANT_COLLECTION_NAME=discharge_documents

# Optional: AI (RAG answers)
OPENAI_API_KEY=
GEMINI_API_KEY=
